{
  "output": "build/skeleton.phar",
  "chmod": "0755",
  "compactors": [
    "Herrera\\Box\\Compactor\\Php"
  ],
  "extract": false,
  "files": [
    "bin/skeleton",
    "LICENSE.md",
    "README.md"
  ],
  "finder": [
    {
      "name": ["*.php", "/.*\\.twig$/"],
      "ignoreDotFiles": 0,
      "exclude": [
        "tests"
      ],
      "in": [
        "templates",
        "vendor",
        "src"
      ]
    }
  ],
  "git-commit": "git-commit",
  "stub": "bin/phar-stub.php",
  "web": false
}